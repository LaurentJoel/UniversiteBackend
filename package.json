{
  "name": "university-room-management",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:prod": "eas build --profile production --platform all",
    "build:ios": "eas build --profile production --platform ios",
    "build:android": "eas build --profile production --platform android",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "npm run submit:ios && npm run submit:android",
    "update": "eas update --auto",
    "deploy:dev": "powershell -ExecutionPolicy Bypass -File .\\deploy.ps1 development all",
    "deploy:preview": "powershell -ExecutionPolicy Bypass -File .\\deploy.ps1 preview all",
    "deploy:prod": "powershell -ExecutionPolicy Bypass -File .\\deploy.ps1 production all"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "expo": "~53.0.0",
    "expo-status-bar": "~2.2.3",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^25.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.5.3",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.20.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}
